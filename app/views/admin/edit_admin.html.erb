<h1>Edit Admin</h1>

<%= form_tag :controller => 'admin', :action => 'edit_admin' do%>


    <p>

      <label for = "Username">Username</label>
      <%= text_field_tag :username,params[:username],:value=> User.find(params[:id]).username  %>  <br/>
      <label for = "password">password</label>
      <%= text_field_tag :password,params[:password], :value => User.find(params[:id]).password  %>
    </p>

    <p>
      <%= submit_tag "Update"  %>
    </p>

<%end %>